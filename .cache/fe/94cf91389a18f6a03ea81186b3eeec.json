 : (\n      <Fragment>\n        {isMobile ? (\n          <ModalForM {...this.props} projects={projects} />\n        ) : (\n          <ModalForPC {...this.props} projects={projects} />\n        )}\n      </Fragment>\n    );\n  }\n}\n\nexport default AddDialog;\n"},"lineCount":null}},"error":null,"hash":"fa105715d6f42cfd74a9389400d6d83d","cacheData":{"env":{}}}
nerated":{"line":1,"column":2459}},{"source":"src/components/Load/index.jsx","original":{"line":103,"column":28},"generated":{"line":1,"column":2469}},{"source":"src/components/Load/index.jsx","name":"PropTypes","original":{"line":103,"column":29},"generated":{"line":1,"column":2470}},{"source":"src/components/Load/index.jsx","name":"string","original":{"line":103,"column":39},"generated":{"line":1,"column":2472}},{"source":"src/components/Load/index.jsx","name":"string","original":{"line":103,"column":39},"generated":{"line":1,"column":2480}},{"source":"src/components/Load/index.jsx","name":"PropTypes","original":{"line":103,"column":47},"generated":{"line":1,"column":2487}},{"source":"src/components/Load/index.jsx","name":"number","original":{"line":103,"column":57},"generated":{"line":1,"column":2489}},{"source":"src/components/Load/index.jsx","name":"number","original":{"line":103,"column":57},"generated":{"line":1,"column":2497}}],"sources":{"src/components/Load/index.jsx":"﻿import PropTypes from \"prop-types\";\n\nimport React from \"react\";\nimport classNames from \"classnames\";\nimport styled from \"styled-components\";\nconst MdLoader = styled.div`\n   {\n    &.MdLoader {\n      position: relative;\n      margin: 0 auto;\n      .circle(@size: 88px) {\n        width: @size;\n        height: @size;\n        border-radius: 50%;\n      }\n    }\n\n    &.MdLoader:before {\n      content: \"\";\n      display: block;\n      padding-top: 100%;\n    }\n\n    .MdLoader-circular {\n      animation: mdLoaderRotate 2s linear infinite;\n      height: 100%;\n      transform-origin: center center;\n      width: 100%;\n      position: absolute;\n      top: 0;\n      bottom: 0;\n      left: 0;\n      right: 0;\n      margin: auto;\n    }\n\n    @keyframes mdLoaderRotate {\n      to {\n        transform: rotate(1turn);\n      }\n    }\n\n    .MdLoader-path {\n      stroke: #1e88e5;\n      stroke-dasharray: 1, 200;\n      stroke-dashoffset: 0;\n      stroke-linecap: round;\n      stroke-miterlimit: 10;\n      fill: none;\n      animation: mdLoaderDash 1.5s ease-in-out infinite;\n    }\n\n    @keyframes mdLoaderDash {\n      0% {\n        stroke-dasharray: 1, 200;\n        stroke-dashoffset: 0;\n      }\n\n      50% {\n        stroke-dasharray: 89, 200;\n        stroke-dashoffset: -35px;\n      }\n\n      to {\n        stroke-dasharray: 89, 200;\n        stroke-dashoffset: -124px;\n      }\n    }\n  }\n`;\n/**\n * 加载中\n */\nexport default function Load(props) {\n  let { size, style } = props;\n  if (size === \"big\") {\n    size = 36;\n  } else if (!size || size === \"middle\") {\n    size = 24;\n  } else if (size === \"small\") {\n    size = 16;\n  }\n  const strokeWidth = Math.floor(size / 8);\n  const r = Math.floor(size / 2);\n  const cx = r + strokeWidth;\n  const cy = cx;\n  const width = cx + cy;\n  return (\n    <div\n      style={style}\n      className={classNames(\"divCenter TxtCenter TxtMiddle\", props.className)}\n    >\n      <MdLoader className=\"MdLoader\" style={{ width }}>\n        <svg className=\"MdLoader-circular\">\n          <circle {...{ cx, cy, r, strokeWidth }} className=\"MdLoader-path\" />\n        </svg>\n      </MdLoader>\n    </div>\n  );\n}\nLoad.propTypes = {\n  className: PropTypes.string,\n  size: PropTypes.oneOfType([PropTypes.string, PropTypes.number]),\n};\n"},"lineCount":null}},"error":null,"hash":"36b748ce7bf26d759a551ccf07375283","cacheData":{"env":{}}}